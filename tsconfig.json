{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "es6", "esnext", "DOM.Iterable"],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "isolatedModules": true,
        "jsx": "react-jsx"
    },
    "include": [
        "src"
, "pages/api"    ],
    "exclude": [
        "node_modules"
    ]
}
